
Syntax * test(Syntax * in, Environ * env) {
  Mark * fluid mark = new_mark;
  Syntax *a, *b;
  match(`[$a,$b], in);
  printf("---\n");
  Syntax * c = `{1 + $a};
  Syntax * d = `(plus $(b) $c);
  Syntax * e = `{$(d) + 8};
  return e;
}

make_macro test;

Syntax * test2(Syntax * in, Environ * env) {
  Mark * fluid mark = new_mark;
  Syntax *name, *val;
  match(`[$name,$val], in);
  return `{int $name() {return $val;}};
}

make_macro test2;

test2(myfun,34);

int main() {
  printf("%d\n", test(2,4));
  printf("%d\n", myfun());
  return 0;
}

